# Green Environment Configuration
NODE_ENV=production
IMAGE_TAG=green-latest

# API Configuration
API_KEY=dinoair-green-api-key-secure-token
API_SECRET=green-environment-secret-key-change-in-production

# Session and Security
SESSION_SECRET=green-session-secret-key-minimum-32-chars
JWT_SECRET=green-jwt-secret-key-for-authentication

# CORS Configuration
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# ComfyUI Configuration
COMFYUI_WORKFLOW_PATH=./FreeTierPacked/sd15-workflow.json

# Application Configuration
NEXT_PUBLIC_APP_NAME=DinoAir Green Environment
NEXT_PUBLIC_VERSION=1.0.0
NEXT_PUBLIC_IS_FREE_TIER=true
NEXT_PUBLIC_ENVIRONMENT=green

# Database Configuration (for future use)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=dinoair_green
DB_USER=dinoair_green
DB_PASSWORD=secure-green-db-password

# Redis Configuration (for future use)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=secure-green-redis-password