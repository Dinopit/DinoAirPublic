# Blue Environment Configuration
NODE_ENV=production
IMAGE_TAG=blue-latest

# API Configuration
API_KEY=dinoair-blue-api-key-secure-token
API_SECRET=blue-environment-secret-key-change-in-production

# Session and Security
SESSION_SECRET=blue-session-secret-key-minimum-32-chars
JWT_SECRET=blue-jwt-secret-key-for-authentication

# CORS Configuration
CORS_ORIGIN=*

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# ComfyUI Configuration
COMFYUI_WORKFLOW_PATH=./FreeTierPacked/sd15-workflow.json

# Application Configuration
NEXT_PUBLIC_APP_NAME=DinoAir Blue Environment
NEXT_PUBLIC_VERSION=1.0.0
NEXT_PUBLIC_IS_FREE_TIER=true
NEXT_PUBLIC_ENVIRONMENT=blue

# Database Configuration (for future use)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=dinoair_blue
DB_USER=dinoair_blue
DB_PASSWORD=secure-blue-db-password

# Redis Configuration (for future use)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=secure-blue-redis-password