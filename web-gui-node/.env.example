# DinoAir Web GUI Environment Configuration
# WARNING: This file should NOT contain real credentials
# Copy .env.example and fill in your own values

# Server Configuration
PORT=3000
NODE_ENV=development
PUBLIC_URL=http://localhost:3000
CORS_ORIGIN=http://localhost:3000

# Supabase Configuration - REPLACE WITH YOUR VALUES
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Database Configuration - REPLACE WITH YOUR VALUES
DATABASE_URL=postgresql://username:password@host:port/database

# Session Configuration - REPLACE WITH YOUR VALUES
SESSION_SECRET=your_secure_session_secret_here

# API Configuration
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Socket.io Configuration
SOCKET_CORS_ORIGIN=http://localhost:3000

# Supabase API Keys - REPLACE WITH YOUR VALUES
SUPABASE_API_KEY=your_supabase_api_key_here
SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_here
SUPABASE_SECRET_KEY=your_supabase_secret_key_here