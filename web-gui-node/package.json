{
  "name": "dinoair-web-gui-node",
  "version": "1.0.0",
  "description": "DinoAir Web GUI - Node.js version with Express server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:contracts:provider": "jest --config jest.contracts.config.js",
    "contracts:verify": "npm run test:contracts:provider",
    "lint": "eslint .",
    "build": "echo 'No build step required for Node.js server'",
    "db:setup": "node scripts/setup-database.js",
    "db:test": "node scripts/test-supabase.js",
    "test:crud": "node scripts/test-crud-operations.js",
    "test:supabase": "npm run db:test && npm run test:crud",
    "load-test": "./load-tests/run-all-tests.sh",
    "load-test:quick": "./load-tests/quick-test.sh",
    "load-test:chat": "npx artillery run load-tests/scenarios/chat-api.yml",
    "load-test:health": "npx artillery run load-tests/scenarios/health-api.yml",
    "load-test:artifacts": "npx artillery run load-tests/scenarios/artifacts-api.yml"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "eventsource": "^4.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lru-cache": "^11.1.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "prismjs": "^1.30.0",
    "socket.io": "^4.7.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@pact-foundation/pact": "^13.1.0",
    "artillery": "^2.0.21",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "dinoair",
    "ai-assistant",
    "web-gui",
    "chat-interface",
    "express",
    "nodejs"
  ],
  "author": "DinoAir Team",
  "license": "MIT"
}
