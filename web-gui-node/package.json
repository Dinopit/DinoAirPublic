{
  "name": "dinoair-web-gui-node",
  "version": "1.0.0",
  "description": "DinoAir Web GUI - Node.js version with Express server",
  "main": "server.js",
  "scripts": {
    "start": "node --trace-warnings server.js",
    "dev": "nodemon --trace-warnings server.js",
    "test": "jest",
    "test:contracts:provider": "jest --config jest.contracts.config.js",
    "contracts:verify": "npm run test:contracts:provider",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:errors": "eslint . --quiet",
    "lint:errors:fix": "eslint . --quiet --fix",
    "lint:staged": "eslint $(git diff --cached --name-only --relative | grep -E '\\.(js|jsx)$' | xargs)",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write $(git diff --cached --name-only --relative | grep -E '\\.(js|jsx|json|md|yml|yaml|css|scss)$' | xargs)",
    "quality": "npm run lint && npm run format:check && npm run test",
    "quality:fix": "npm run lint:fix && npm run format && npm run test",
    "quality:quick": "npm run lint:errors && npm run format:check",
    "quality:minimal": "npm run format:check",
    "pre-commit": "lint-staged && npm run quality",
    "build": "echo 'No build step required for Node.js server'",
    "db:setup": "node scripts/setup-database.js",
    "db:test": "node scripts/test-supabase.js",
    "test:crud": "node scripts/test-crud-operations.js",
    "test:supabase": "npm run db:test && npm run test:crud",
    "load-test": "./load-tests/run-all-tests.sh",
    "load-test:quick": "./load-tests/quick-test.sh",
    "load-test:chat": "npx artillery run load-tests/scenarios/chat-api.yml",
    "load-test:health": "npx artillery run load-tests/scenarios/health-api.yml",
    "load-test:artifacts": "npx artillery run load-tests/scenarios/artifacts-api.yml",
    "load-test:auth": "npx artillery run load-tests/scenarios/auth-api.yml",
    "load-test:system": "npx artillery run load-tests/scenarios/system-api.yml",
    "load-test:all": "npm run load-test:health && npm run load-test:chat && npm run load-test:artifacts && npm run load-test:auth && npm run load-test:system",
    "security:validate": "node scripts/validate-security.js",
    "security:test": "npm run test && npm run security:validate",
    "prepare": "cd .. && husky install",
    "lint:fix-safe": "npx eslint --fix . || true"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/instrumentation-express": "^0.52.0",
    "@opentelemetry/instrumentation-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@supabase/supabase-js": "^2.52.1",
    "apollo-server-express": "^3.12.1",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "eventsource": "^4.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.2.1",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^9.0.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lru-cache": "^11.1.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "prismjs": "^1.30.0",
    "qrcode": "^1.5.4",
    "semver": "^7.6.3",
    "socket.io": "^4.7.4",
    "speakeasy": "^2.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@faker-js/faker": "^9.9.0",
    "@pact-foundation/pact": "^13.1.0",
    "artillery": "^2.0.23",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "dinoair",
    "ai-assistant",
    "web-gui",
    "chat-interface",
    "express",
    "nodejs"
  ],
  "author": "DinoAir Team",
  "license": "MIT"
}
